<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
        
    <SolidColorBrush x:Key="SvgLeftMenuColor">#d0dbe8</SolidColorBrush>
    <SolidColorBrush x:Key="PrimaryForegroundColor">#333333</SolidColorBrush>
    <SolidColorBrush x:Key="PrimaryBackgroundColor" Color="#ffffff" />
    <SolidColorBrush x:Key="SecondaryBackgroundColor" Color="#f1f1f1"/>
    <LinearGradientBrush x:Key="LeftMenuBackground" StartPoint="0,0" EndPoint="0,0.06" >
        <GradientStop Color="#f1f1f1" Offset="0"/>
        <GradientStop Color="#fff" Offset="1"/>
    </LinearGradientBrush>

</ResourceDictionary>