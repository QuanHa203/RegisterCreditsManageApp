    <!-- Alert Window -->
    <Style x:Key="AlertDangerBtn" TargetType="Button">
        <Setter Property="Foreground" Value="#fff"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate>
                    <Border CornerRadius="4" Background="#f03537" Width="auto" Height="30" HorizontalAlignment="Center">

                        <ContentPresenter>
                            <TextBlock x:Name="Text" Padding="12 0 12 0" HorizontalAlignment="Center" VerticalAlignment="Center">Ok</TextBlock>
                        </ContentPresenter>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

