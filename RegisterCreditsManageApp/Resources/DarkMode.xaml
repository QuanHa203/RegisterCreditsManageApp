            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="AlertWarningBtn" TargetType="Button">
        <Setter Property="Foreground" Value="#ffae00"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate>
                    <Border CornerRadius="4" Background="Transparent" Width="auto" Height="30" HorizontalAlignment="Center" BorderBrush="#ffae00">
