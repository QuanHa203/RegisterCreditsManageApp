    <!-- Alert Window -->
    <Style x:Key="AlertDangerBtn" TargetType="Button">
        <Setter Property="Foreground" Value="#f03537"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate>
                    <Border CornerRadius="4" Background="Transparent" Width="auto" Height="30" HorizontalAlignment="Center" BorderBrush="#f03537" BorderThickness="1">
                        <TextBlock Padding="12 0 12 0" HorizontalAlignment="Center" VerticalAlignment="Center">Ok</TextBlock>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="AlertInfoBtn" TargetType="Button">
        <Setter Property="Foreground" Value="#3663fd"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate>
                    <Border CornerRadius="4" Background="Transparent" Width="auto" Height="30" HorizontalAlignment="Center" BorderBrush="#3663fd">
                        <TextBlock Padding="12 0 12 0" HorizontalAlignment="Center" VerticalAlignment="Center">Ok</TextBlock>
                    </Border>
