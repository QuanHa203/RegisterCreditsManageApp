    <SolidColorBrush x:Key="ThirdForegroundColor">#99acc2</SolidColorBrush>
    
    <SolidColorBrush x:Key="PrimaryBackgroundColor" Color="#1c2340" />
    <SolidColorBrush x:Key="SecondaryBackgroundColor" Color="#151934"/>


    <SolidColorBrush x:Key="SearchIconFill" Color="#9b69ea"/>
    <FontFamily x:Key="DefaultFontFamily">Montserrat</FontFamily>
    <CornerRadius x:Key="DefaultPrimaryCornerRadius">12</CornerRadius>
    <CornerRadius x:Key="DefaultSecondaryCornerRadius">6</CornerRadius>
    <SolidColorBrush x:Key="DefaultFocusForeground" Color="#3663fe"/>
    <SolidColorBrush x:Key="DefaultBackgroundDataGridColumnHeader" Color="#252d4c"/>
